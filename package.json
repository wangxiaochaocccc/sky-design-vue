{
  "name": "sky-design-vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "sideEffects": ["dist/index.css"],
  "files": [
    "dist"
  ],
  "main": "./dist/umd/tj-design-vue.umd.cjs",
  "module": "./dist/es/tj-design-vue.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/tj-design-vue.js",
      "require": "./dist/umd/tj-design-vue.umd.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build-only && npm run move-style",
    "build-only": "run-p build-umd build-es",
    "build-umd": "vite build --config vite.umd.config.ts",
    "build-es": "vite build --config vite.es.config.ts",
    "move-style":"move-file  dist/es/index.css dist/index.css",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
   
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitepress-demo-preview/component": "^2.3.2",
    "@vitepress-demo-preview/plugin": "^1.2.2",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.1",
    "move-file-cli": "^3.0.0",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-for": "^2.1.1",
    "typescript": "^4.7.4",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.7.3",
    "vite-plugin-eslint": "^1.8.1",
    "vitepress": "1.0.0-rc.44",
    "vue": "^3.4.15",
    "vue-tsc": "^1.8.27"
  }
}